@model List<DapperExample.Models.EmployeeModel>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

@section mysection
{
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>


}

<table class="table table-bordered table-striped">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>EmpSalary</th>
        <th>Action</th>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = @item.EmpId }, new { @class = "btn btn-success" })
            @Html.ActionLink("Delete", "Delete", new {id=@item.EmpId },new {@class="btn btn-danger" })
            </td>
        </tr>
    }
</table>


 