
@{
    ViewBag.Title = "Index7";
}

<h2>Index7</h2>

<table class="table table-bordered table-striped">

    <tr>
        <th>EmpId</th>
        <th>Name</th>
        <th>Salary</th>
    </tr>


    @foreach (var item in ViewBag.listEmp)//3 employees
    {
        <tr>
            <td>@item.EmpId </td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table>

